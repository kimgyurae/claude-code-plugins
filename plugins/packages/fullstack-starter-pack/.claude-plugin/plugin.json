{
  "name": "fullstack-starter-pack",
  "version": "1.0.0",
  "description": "Complete fullstack development toolkit: React, Express/FastAPI, PostgreSQL scaffolding with AI agents",
  "author": {
    "name": "kimgyurae",
    "email": "[email protected]",
    "url": "https://github.com/kimgyurae"
  },
  "repository": "https://github.com/kimgyurae/claude-code-plugins",
  "homepage": "https://kimgyurae.github.io/claude-code-plugins/",
  "license": "MIT",
  "keywords": [
    "fullstack",
    "react",
    "express",
    "fastapi",
    "postgresql",
    "prisma",
    "typescript",
    "authentication",
    "scaffolding",
    "components",
    "api",
    "database",
    "deployment",
    "docker",
    "ci-cd",
    "mvp"
  ],
  "categories": [
    "frontend",
    "backend",
    "database",
    "devops"
  ],
  "plugins": [
    {
      "name": "react-specialist",
      "type": "agent",
      "path": "plugins/01-frontend/agents/react-specialist.md"
    },
    {
      "name": "ui-ux-expert",
      "type": "agent",
      "path": "plugins/01-frontend/agents/ui-ux-expert.md"
    },
    {
      "name": "component-generator",
      "type": "command",
      "shortcut": "cg",
      "path": "plugins/01-frontend/commands/component-generator.md"
    },
    {
      "name": "css-utility-generator",
      "type": "command",
      "shortcut": "cug",
      "path": "plugins/01-frontend/commands/css-utility-generator.md"
    },
    {
      "name": "api-builder",
      "type": "agent",
      "path": "plugins/02-backend/agents/api-builder.md"
    },
    {
      "name": "backend-architect",
      "type": "agent",
      "path": "plugins/02-backend/agents/backend-architect.md"
    },
    {
      "name": "express-api-scaffold",
      "type": "command",
      "shortcut": "eas",
      "path": "plugins/02-backend/commands/express-api-scaffold.md"
    },
    {
      "name": "fastapi-scaffold",
      "type": "command",
      "shortcut": "fas",
      "path": "plugins/02-backend/commands/fastapi-scaffold.md"
    },
    {
      "name": "database-designer",
      "type": "agent",
      "path": "plugins/03-database/agents/database-designer.md"
    },
    {
      "name": "prisma-schema-gen",
      "type": "command",
      "shortcut": "psg",
      "path": "plugins/03-database/commands/prisma-schema-gen.md"
    },
    {
      "name": "sql-query-builder",
      "type": "command",
      "shortcut": "sqb",
      "path": "plugins/03-database/commands/sql-query-builder.md"
    },
    {
      "name": "deployment-specialist",
      "type": "agent",
      "path": "plugins/04-integration/agents/deployment-specialist.md"
    },
    {
      "name": "auth-setup",
      "type": "command",
      "shortcut": "as",
      "path": "plugins/04-integration/commands/auth-setup.md"
    },
    {
      "name": "env-config-setup",
      "type": "command",
      "shortcut": "ecs",
      "path": "plugins/04-integration/commands/env-config-setup.md"
    },
    {
      "name": "project-scaffold",
      "type": "command",
      "shortcut": "ps",
      "path": "plugins/04-integration/commands/project-scaffold.md"
    }
  ],
  "documentation": {
    "installation": "docs/INSTALLATION.md",
    "quickstart": "docs/QUICK_START.md",
    "useCases": "docs/USE_CASES.md",
    "troubleshooting": "docs/TROUBLESHOOTING.md",
    "readme": "docs/README.md"
  },
  "requirements": {
    "claudeCode": ">=0.1.0",
    "optional": {
      "nodejs": ">=18.0.0",
      "python": ">=3.10",
      "docker": ">=20.10",
      "postgresql": ">=14.0"
    }
  }
}

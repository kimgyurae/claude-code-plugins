{
  "name": "security-pro-pack",
  "version": "1.0.0",
  "description": "Professional security tools for Claude Code: vulnerability scanning, compliance, cryptography audit, container & API security",
  "author": {
    "name": "kimgyurae",
    "email": "[email protected]",
    "url": "https://github.com/kimgyurae"
  },
  "repository": "https://github.com/kimgyurae/claude-code-plugins",
  "homepage": "https://kimgyurae.github.io/claude-code-plugins/",
  "license": "MIT",
  "keywords": [
    "security",
    "vulnerability-scanning",
    "compliance",
    "HIPAA",
    "PCI-DSS",
    "GDPR",
    "SOC2",
    "cryptography",
    "crypto-audit",
    "OWASP",
    "penetration-testing",
    "container-security",
    "docker-security",
    "API-security",
    "threat-modeling",
    "STRIDE",
    "devsecops"
  ],
  "categories": [
    "security",
    "compliance",
    "devops"
  ],
  "plugins": [
    {
      "name": "security-auditor-expert",
      "type": "agent",
      "path": "plugins/01-core-security/agents/security-auditor-expert.md"
    },
    {
      "name": "penetration-tester",
      "type": "agent",
      "path": "plugins/01-core-security/agents/penetration-tester.md"
    },
    {
      "name": "security-scan-quick",
      "type": "command",
      "shortcut": "ss",
      "path": "plugins/01-core-security/commands/security-scan-quick.md"
    },
    {
      "name": "compliance-checker",
      "type": "agent",
      "path": "plugins/02-compliance/agents/compliance-checker.md"
    },
    {
      "name": "compliance-docs-generate",
      "type": "command",
      "shortcut": "cdg",
      "path": "plugins/02-compliance/commands/compliance-docs-generate.md"
    },
    {
      "name": "crypto-expert",
      "type": "agent",
      "path": "plugins/03-cryptography/agents/crypto-expert.md"
    },
    {
      "name": "crypto-audit",
      "type": "command",
      "shortcut": "ca",
      "path": "plugins/03-cryptography/commands/crypto-audit.md"
    },
    {
      "name": "threat-modeler",
      "type": "agent",
      "path": "plugins/04-infrastructure-security/agents/threat-modeler.md"
    },
    {
      "name": "docker-security-scan",
      "type": "command",
      "shortcut": "dss",
      "path": "plugins/04-infrastructure-security/commands/docker-security-scan.md"
    },
    {
      "name": "api-security-audit",
      "type": "command",
      "shortcut": "asa",
      "path": "plugins/04-infrastructure-security/commands/api-security-audit.md"
    }
  ],
  "documentation": {
    "installation": "docs/INSTALLATION.md",
    "quickstart": "docs/QUICK_START.md",
    "useCases": "docs/USE_CASES.md",
    "troubleshooting": "docs/TROUBLESHOOTING.md",
    "readme": "docs/README.md"
  },
  "requirements": {
    "claudeCode": ">=0.1.0",
    "optional": {
      "docker": ">=20.10",
      "python": ">=3.8",
      "nodejs": ">=16"
    }
  }
}
